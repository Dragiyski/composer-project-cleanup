{
    "name": "dragiyski/composer-project-cleanup",
    "version": "0.0.1",
    "description": "Allows cleaning up files from the project after composer install/update.",
    "type": "composer-plugin",
    "require": {
        "ext-json": "*",
        "composer-plugin-api": "^1.0"
    },
    "extra": {
        "class": "Dragiyski\\Composer\\ProjectCleanup\\Plugin"
    },
    "autoload": {
        "psr-0": {
            "Dragiyski\\Composer\\ProjectCleanup": "src/"
        }
    }
}